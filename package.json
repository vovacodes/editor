{
  "name": "editor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && cp -r build build-preview && rm -f build-preview/index.html",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "yarn deploy:editor && yarn deploy:preview",
    "deploy:editor": "netlify deploy -d build --site 61919525-545f-4c3a-9daa-c30ea714d141 --prod",
    "deploy:preview": "netlify deploy -d build-preview --site cc356a62-f0a3-408d-b992-1cf803749483 --prod"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "comlink": "^4.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "netlify-cli": "^2.47.0",
    "typescript": "~3.8.3",
    "worker-loader": "^2.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
